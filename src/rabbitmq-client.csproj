<Project Sdk="Microsoft.NET.Sdk">
  <PropertyGroup>
    <TargetFramework>net6.0</TargetFramework>
    <RootNamespace>rabbitmq_client</RootNamespace>
    <ImplicitUsings>enable</ImplicitUsings>
    <Nullable>enable</Nullable>
  </PropertyGroup>
    <ItemGroup>
    <EmbeddedResource Include="resources\rabbitmq-logo.png">
      <LogicalName>psn.PH.RabbitMQClientExtIcon.png</LogicalName>
    </EmbeddedResource>
    <EmbeddedResource Include="resources\buildinfo.txt">
      <LogicalName>psn.PH.buildinfo.txt</LogicalName>
    </EmbeddedResource>    
    <Content Include="resources\*.*">
      <CopyToOutputDirectory>Always</CopyToOutputDirectory>
    </Content>
  </ItemGroup>
  <ItemGroup>
    <PackageReference Include="OutSystems.ExternalLibraries.SDK" Version="*"/>
    <PackageReference Include="Newtonsoft.Json" Version="*" />
    <PackageReference Include="RabbitMQ.Client" Version="*" />
  </ItemGroup>
</Project>
